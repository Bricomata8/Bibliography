  author = {Kobo, H. I. and Abu-Mahfouz, A. M. and Hancke, G. P.},
  file = {/home/aghiles/Aghiles/Redaction/setup/lib/iot/storage/9RW9ERJD/Kobo et al_2017_A Survey on Software-Defined Wireless Sensor Networks.pdf},
