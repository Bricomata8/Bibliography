@inproceedings{magno_ultra_2014,
  langid = {english},
  location = {{Larnaca, Cyprus}},
  title = {An Ultra Low Power High Sensitivity Wake-up Radio Receiver with Addressing Capability},
  isbn = {978-1-4799-5041-6},
