  author = {Lu, Lingling and University, Zhejiang and Chen, Jianhai and University, Zhejiang and Tian, Zijun and University, Zhejiang and He, Qinming and University, Zhejiang and Huang, Butian and University, Zhejiang and Xiang, Yang},
  file = {Lu et al. - EduCoin a Secure and Efficient Payment Solution f.pdf:/home/aghiles/Aghiles/Redaction/lib/iot/storage/QF64KNPD/Lu et al. - EduCoin a Secure and Efficient Payment Solution f.pdf:application/pdf},
