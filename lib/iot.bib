  groups = {houda}
}

@article{guntupalli_energy_2018,
  timestamp = {2019-06-28T16:46:35Z},
  langid = {english},
  title = {Energy {{Efficient Consecutive Packet Transmissions}} in {{Receiver}}-{{Initiated Wake}}-{{Up Radio Enabled WSNs}}},
