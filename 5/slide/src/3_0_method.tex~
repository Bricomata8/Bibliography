\subsection{Contagion process}

\begin{frame}{... (step 1)}{Methods}

	\Columns{0.5}{0.5}{
		\Itemize{
			\item ...
				\Itemize{
					\item ...
					\item ...
					\item ...
				}
			\item ...
				\Itemize{
					\item ...
					\item ...
					\item ...
				}
		}
	}{
		\Figure{h}{.7}{vulnerability_.png}{Vulnerability contagion process}
%		\movie[autostart,label=cells,width=\textwidth,height=0.5\textheight,showcontrols]{}{movie.mp4}
	}

	\note{
		\begin{itemize}
			\item Motiver les étapes, les hypothèses, le contexte
			\item Illustrer par des schémas et figures
			\item Se concentrer sur les aspects où l’approche apporte une contribution
			\item Justification du choix de l'approche
			\item Mise en œuvre des hypothèses
			\item Montrez les modules/algorithmes pris de l’existant, ne réinventez pas la roue.
			
			\item Description de la solution
				\begin{itemize}
					\item Description générale
						\begin{itemize}
							\item approche(hypothèses, paramètres) = résultats
						\end{itemize}
					\item Description détaillée (même exemple)
						\begin{itemize}
							\item (hypothèses, paramètres) -> (étape\_1 -> ... -> étape\_n) -> résultats
						\end{itemize}
				\end{itemize}
			
		\end{itemize}
	}
\end{frame}


