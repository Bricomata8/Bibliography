

\begin{frame}{Etape 3: Calcule de la vulnérabilité sociale}{Théorie de l'influence sociale de Freidkin}

	\Columns{.65}{.35}{
		\Itemize{
			\item Entrée:
			\Itemize{
				\item $Y^{(1)}$ = Un vecteur des vulnérabilités individuelles de N utilisateurs (eq \ref{eq:vulnerability})
				\item \alpha = Le niveau de réputation de chaque utilisateur (eq \ref{eq:1})
				\item M  = une matrice d'adjacence N x N
			}
			\item Modèle:
			\Itemize{
				\item[] \Equation{2}{Y^{(t)}=\alpha MY^{(t-1)} + (1 - \alpha) Y^{(t-1)}}
			}
			\item Sortie:
			\Itemize{
				\item  $Y^{(t)}$ = Un vecteur des vulnérabilités sociales des N utilisateurs
			}
		}
	}{
		\Figure{h}{.73}{oo.png}{Vulnérabilité Sociale}
%		\movie[autostart,label=cells,width=\textwidth,height=0.5\textheight,showcontrols]{}{movie.mp4}
	}

\end{frame}

